@model Prototype.UI.Models.EmployeeListViewModel

@{
    ViewBag.Title = "List";
}

<h2>Employees</h2>

<table class="table">
    <tr>
        <th>Employee Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>SSN</th>
    </tr>
@if (Model != null && Model.Employees != null && Model.Employees.Count > 0)
{

    foreach (var item in Model.Employees)
    {
    <tr>
        <td>@item.EmployeeId</td>
        <td>@item.FirstName</td>
        <td>@item.LastName</td>
        <td>@item.SSN</td>
    </tr>
    }
}
</table>
